save-ch5:  711-cleanup
	@$(call echo_message, Building)
	@export BASHBIN=$(SHELL) && $(SHELL) progress_bar.sh $@ $$PPID &
	@echo "$(nl_)`date`$(nl_)" >logs/$@
	@ : > envars
	@mkdir -p /tmp >>logs/$@ 2>&1 && \
	TARNAME=chapter5-$$(date +%Y-%m-%d-T-%Hh%M).tar && \
        TMPFILE=$$(mktemp -p /tmp) && \
        TMPLOG=$$(mktemp -p /tmp) && \
	for F in ./dev ./home ./lost+found ./media ./mnt ./opt ./proc ./root ./run ./srv ./sys ./tmp ./var; do echo $$F >> $$TMPFILE; done && \
	tar -X $$TMPFILE -cvf /tmp/$$TARNAME -C / . >>$$TMPLOG 2>>logs/$@ && \
	cat $$TMPLOG >>logs/$@ 2>&1 && \
	mv /tmp/$$TARNAME /jhalfs >>logs/$@ 2>&1 && \
	rm $$TMPFILE $$TMPLOG
	@$(call housekeeping)

